{
  "module": "nodenext",
  "target": "es2017",
  "allowJs": true,
  "skipLibCheck": true,
  "strict": true,
  "noUnusedLocals": true,
  "noUnusedParameters": true,
  "forceConsistentCasingInFileNames": true,
  "noEmit": true,
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "resolveJsonModule": true,
  "isolatedModules": true,
  "jsx": "preserve",
  "noImplicitAny": true,
  "incremental": true,
  "strictBindCallApply": true,
  "exclude": [
    "node_modules"
  ],
}
