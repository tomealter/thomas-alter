{
  "name": "nextjs-project",
  "version": "2.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "lint": "next lint && stylelint \"source/**/*.css\"",
    "prepare": "husky",
    "prettier": "prettier --check pages source/",
    "prettier:write": "prettier --write app source/",
    "tsc": "tsc --noEmit",
    "test": "npm run lint && npm run tsc",
    "component": "plop",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o storybook",
    "build-icons": "svgr --out-dir source/01-global/icon/icons -- source/01-global/icon/svgs"
  },
  "browserslist": [
    "last 2 versions and not dead",
    ">= 1%",
    ">= 1% in US"
  ],
  "dependencies": {
    "next": "15.3.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-select": "^5.10.1"
  },
  "devDependencies": {
    "@csstools/postcss-global-data": "^3.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@forumone/eslint-config-es5": "^3.0.0",
    "@forumone/eslint-config-react": "^3.0.0",
    "@next/eslint-plugin-next": "15.3.3",
    "@storybook/addon-a11y": "^9.0.4",
    "@storybook/addon-docs": "^9.0.4",
    "@storybook/addon-links": "^9.0.4",
    "@storybook/nextjs": "^9.0.4",
    "@svgr/babel-plugin-add-jsx-attribute": "^8.0.0",
    "@svgr/babel-plugin-remove-jsx-attribute": "^8.0.0",
    "@svgr/cli": "^8.1.0",
    "@types/node": "^20.17.57",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "clsx": "^2.1.1",
    "css-loader": "^7.1.2",
    "csstype": "3.1.3",
    "enhanced-resolve": "^5.18.1",
    "eslint": "^9.28.0",
    "html-react-parser": "^5.2.5",
    "husky": "^9.1.7",
    "inquirer": "^10.2.2",
    "plop": "^4.0.1",
    "postcss": "^8.5.4",
    "postcss-advanced-variables": "github:kmonahan/postcss-advanced-variables",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-nesting": "^13.0.1",
    "postcss-preset-env": "^10.2.0",
    "postcss-rem": "^3.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "storybook": "^9.0.4",
    "style-loader": "^4.0.0",
    "stylelint": "^16.20.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-webpack-plugin": "^5.0.1",
    "typescript": "~5.8.3",
    "yaml": "^2.8.0"
  },
  "overrides": {
    "csstype": "3.1.3",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "storybook": "$storybook"
  }
}
